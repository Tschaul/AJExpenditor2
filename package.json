{
  "name": "ajexpeditor2",
  "version": "1.0.0",
  "description": "Rewrite of AJExpenditor",
  "main": "migrate.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "node node_modules/babel-cli/bin/babel.js --plugins transform-react-jsx html/lib --out-dir html/build",
    "buildw": "node node_modules/babel-cli/bin/babel.js --watch --plugins transform-react-jsx html/lib --out-dir html/build",
    "push": "jspm bundle-sfx build/main.js ajexpenditor/_attachments/index.js && cd ajexpenditor && couchapp push ajexpenditor && cd ..",
    "serve": "node node_modules/serve/bin/serve.js html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tschaul/AJExpenditor2.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tschaul/AJExpenditor2/issues"
  },
  "homepage": "https://github.com/Tschaul/AJExpenditor2#readme",
  "dependencies": {
    "nano": "^6.4.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "html"
    },
    "dependencies": {
      "pouchdb": "npm:pouchdb@^6.3.2",
      "react": "npm:react@^15.6.1",
      "react-bootstrap": "npm:react-bootstrap@^0.31.1",
      "react-dom": "npm:react-dom@^15.6.1"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    },
    "buildConfig": {
      "transpileES6": true,
      "minify": true
    }
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "serve": "^6.0.2"
  }
}
